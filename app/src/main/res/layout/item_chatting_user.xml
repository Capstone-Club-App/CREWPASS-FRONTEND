<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/item_apply_curr_iv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/round_square_light_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/item_profile_img"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginLeft="26dp"
        android:background="@drawable/round_image"
        app:layout_constraintBottom_toBottomOf="@id/item_apply_curr_iv"
        app:layout_constraintStart_toStartOf="@+id/item_apply_curr_iv"
        app:layout_constraintTop_toTopOf="@id/item_apply_curr_iv" />

    <TextView
        android:id="@+id/txt_club_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:text="익명1"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/item_profile_img"
        app:layout_constraintTop_toTopOf="@id/item_apply_curr_iv"
        app:layout_constraintBottom_toBottomOf="@id/item_apply_curr_iv"/>






</androidx.constraintlayout.widget.ConstraintLayout>